{
  "name": "qejk-backend",
  "version": "0.0.1",
  "description": "My node js app inspired on kwejk.pl",
  "main": "app.js",
  "scripts": {
    "build": "rimraf ./dist && babel ./src -d ./dist && babel index.js --out-file server.js ",
    "prestart": "npm run -s build",
    "dev": "nodemon -L --inspect=0.0.0.0:3030 --exec babel-node index.js",
    "dev-local": "nodemon --exec babel-node index.js",
    "start": "node server.js --bind 0.0.0.0:$PORT"
  },
  "author": "Karwowski Adrian",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-joi-validation": "^0.3.0",
    "imgur-uploader": "^4.2.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "mime": "^2.4.0",
    "mongoose": "^5.4.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-import-rename": "^1.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "rimraf": "^2.6.3"
  }
}
